{
  "name": "TechFolioDesigner",
  "productName": "TechFolioDesigner",
  "version": "1.0.16",
  "description": "Desktop application to support development of techfolios.",
  "repository": {
    "type": "git",
    "url": "https://github.com/techfolios/designer.git"
  },
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "philipjohnson",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "osxSign": true
      },
      "electronWinstallerConfig": {
        "name": "<%= productName %>",
        "certificateFile": "./Certificates.p12"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "techfolios",
        "name": "techfoliodesigner",
        "draft": true
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "<%= productName %>"
      }
    }
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clipboardy": "^1.2.3",
    "codemirror": "^5.39.0",
    "codemirror-spell-checker": "^1.1.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.2.4",
    "electron-oauth2": "^3.0.0",
    "electron-prompt": "^1.0.0",
    "electron-redux": "^1.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^2.0.0",
    "is-directory": "^0.3.1",
    "jsonlint": "^1.6.3",
    "markdownlint": "^0.11.0",
    "moment": "^2.22.2",
    "node-notifier": "^5.2.1",
    "notifyjs": "^3.0.0",
    "query-string": "^6.1.0",
    "react": "^15.6.2",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-simplemde-editor": "^3.6.16",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "semantic-ui-css": "^2.3.2",
    "semantic-ui-react": "^0.82.0",
    "simpl-schema": "^1.5.0",
    "simple-git": "^1.96.0",
    "simplemde": "^1.11.2",
    "underscore": "^1.9.1",
    "uniforms": "^1.25.0-rc.3",
    "uniforms-semantic": "^1.25.0-rc.3",
    "vfile-reporter": "^5.0.0",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jshint": "^2.9.6",
    "node-key-sender": "^1.0.10"
  }
}
